      "source": [
        "def mutate_string(string, yer, karakter):\n",
        "    a = list(string)\n",
        "    a[yer] = karakter\n",
        "    string = \"\".join(a)\n",
        "    return string\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    s = input()\n",
        "    i, c = input().split()\n",
        "    s_new = mutate_string(s, int(i), c)\n",
        "    print(s_new)"
      ]
    }
  ]
}